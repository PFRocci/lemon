# Somewhat convoluted test cases

"__--_wiDE___", 'WIDE'
"wide---", 'WIDE'
"_---b- -", 'B'
"___b _-", 'B'
"Z-", 'Z'
"Z-__", 'Z'
"__G", 'G'
"g-", 'G'
"--_-I", 'I'
" i-", 'I'
"h_---", 'H'
"H -", 'H'
"-K ", 'K'
"-__--K--_ ", 'K'
" j", 'J'
" J", 'J'
"_-__-M__", 'M'
"_-m-", 'M'
" l", 'L'
" _L _-", 'L'
" -__N-", 'N'
" -N--__", 'N'
"kS", 'KS'
"KS-", 'KS'
"_r_-_", 'R'
"- ___R-_", 'R'
"  _U", 'U'
" -_U ", 'U'
"-__W-", 'W'
"-  _W--", 'W'
"_-V  -", 'V'
"  v_ _", 'V'
"-_Y_", 'Y'
"Y_", 'Y'
"NARRow_-_", 'NARROW'
"-_narRoW", 'NARROW'
